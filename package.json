{
  "name": "flowmark",
  "main": "./lib/main",
  "version": "0.6.3",
  "description": "An intelligent, collaboration-friendly auto-formatter for Markdown",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "flowmark:standard",
      "flowmark:reformatPlain",
      "flowmark:reformatWrap",
      "flowmark:smartQuotes",
      "flowmark:normalizeFootnotes",
      "flowmark:normalizeHeadings",
      "flowmark:normalizeBoldface",
      "flowmark:heavyCleanup"
    ]
  },
  "repository": "git@github.com:jlevy/atom-flowmark.git",
  "author": "Joshua Levy <josh@holloway.com>",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "mdast-util-to-string": "^1.0.6",
    "remark-frontmatter": "^1.3.1",
    "remark-parse": "^5.0.0",
    "remark-stringify": "^5.0.0",
    "remark-textr": "^3.0.3",
    "textr": "^0.3.0",
    "title-case": "^3.0.2",
    "typographic-quotes": "^2.0.1",
    "unified": "^7.1.0",
    "unist-util-is": "^2.1.2",
    "unist-util-visit": "^1.4.0",
    "unist-util-visit-parents": "^2.0.0"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": true,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "scripts": {
    "format": "xo --prettier --fix",
    "test": "xo"
  },
  "devDependencies": {
    "xo": "^0.25.3"
  }
}
